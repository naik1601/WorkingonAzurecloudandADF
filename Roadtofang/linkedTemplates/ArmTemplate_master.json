{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Roadtofang"
		},
		"AzureDataLakeSg1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeSg1'"
		},
		"AzureSqlDb1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDb1'"
		},
		"AzureDataLakeSg1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://missiontofangadls.dfs.core.windows.net/"
		},
		"AzureSqlDb1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "assignmentserver100.database.windows.net"
		},
		"AzureSqlDb1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Mission100shreydobbb"
		},
		"AzureSqlDb1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azurelib"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Roadtofang_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeSg1_accountKey": {
						"value": "[parameters('AzureDataLakeSg1_accountKey')]"
					},
					"AzureSqlDb1_password": {
						"value": "[parameters('AzureSqlDb1_password')]"
					},
					"AzureDataLakeSg1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeSg1_properties_typeProperties_url')]"
					},
					"AzureSqlDb1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDb1_properties_typeProperties_server')]"
					},
					"AzureSqlDb1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDb1_properties_typeProperties_database')]"
					},
					"AzureSqlDb1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDb1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Roadtofang_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Roadtofang_ArmTemplate_0')]"
			]
		}
	]
}